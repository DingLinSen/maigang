{
  "presets": [
    [
      "env", {
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
      }
    ],
    "stage-2",
    ["es2015", { "modules": false }]
  ],
  "plugins": [
    // "transform-vue-jsx", 
    "transform-runtime",
    "transform-es2015-modules-commonjs",
    // "dynamic-import-node",
    [
      "component",
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      }
    ]
  ]
}
